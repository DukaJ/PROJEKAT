@model IEnumerable<ProjekatFinalni.Models.MailAdresa>

@{
    ViewBag.Title = "Index";
}

<h2>Email</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Adresa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OznakaTipaMail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KontaktID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Adresa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OznakaTipaMail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KontaktID)
        </td>
     
    </tr>
}

</table>
<P STYLE="text-align:right">
    <a href="javascript:window.print()">Odštampaj stranicu</a>
</P>